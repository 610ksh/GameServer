<?xml version="1.0" encoding="utf-8" ?> <!--Header-->
<PDL>
  <!--
    MoveToContent로 아래부터 시작.
    depth = 1임
    r. Name : name의 앞부분 키워드
    r["name"] : r 부분의 name 태그 부분
   -->
  <packet name="PlayerInfoReq">
    <long name="playerId"/>
    <string name="name"/>
    <list name="skill">
      <int name="id"/>
      <short name="level"/>
      <float name="duration"/>
    </list>
  </packet>
</PDL>